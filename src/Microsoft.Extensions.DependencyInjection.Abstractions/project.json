{
  "version": "1.2.0-*",
  "description": "Abstractions for dependency injection.\r\nCommonly used types:\r\nMicrosoft.Extensions.DependencyInjection.IServiceCollection",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/dependencyinjection"
    },
    "tags": [
      "dependencyinjection",
      "di"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.ActivatorUtilities.Sources": {
      "type": "build",
      "version": "1.2.0-*"
    },
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.ComponentModel": "4.4.0-*"
      }
    }
  }
}